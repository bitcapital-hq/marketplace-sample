  "version": "0.2.0",
  "configurations": [
    
    {
      "type": "node",
      "request": "attach",
      "name": "Attach",
      "port": 9229,
    },
    {
      "type": "node",
      "request": "launch",
      "name": "start (ts-node/local)",
      "cwd": "${workspaceRoot}",
      "runtimeExecutable": "/Users/danilomendes/n/bin/node",
      "runtimeArgs": [
        "-r",
        "ts-node/register",
      ],
      "args": [
        "${workspaceFolder}/start.ts"
      ],
      "env": {
        "NODE_ENV": "development",
        "PORT": "3000"
      },
      "sourceMaps": true,
      "internalConsoleOptions": "openOnFirstSessionStart",
      "console": "externalTerminal"
    },
    {
      "name": "Debug Jest Single File",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceRoot}",
      "runtimeExecutable": "/Users/danilomendes/n/bin/node",
      "port": 9229,
      "runtimeArgs": [
        "--require",
        "ts-node/register",
        "--inspect=9229",
        "${workspaceRoot}/node_modules/.bin/jest",
        "${relativeFile}",
        "--runInBand",
        "--config=jest.json"
      ],
      "env": {
      },
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
    },
    {
      "name": "Debug Jest Tests",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceRoot}",
      "runtimeExecutable": "/Users/danilomendes/n/bin/node",
      "port": 9229,
      "runtimeArgs": [
        "--require",
        "ts-node/register",
        "--inspect=9229",
        "${workspaceRoot}/node_modules/.bin/jest",
        "--runInBand",
        "--config=jest.json"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
    }
  ]
}